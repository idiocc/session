{
  "name": "@goa/session",
  "version": "0.0.0-pre",
  "description": "[fork] Simple session middleware for koa",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "depack": "depack-dev src/depack -o depack -a -l -p --externs types/externs.js",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "typal types/index.js types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "depack",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/session.git"
  },
  "keywords": [
    "session",
    "goa"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/session/issues"
  },
  "homepage": "https://github.com/idiocc/session#readme",
  "devDependencies": {
    "@contexts/http": "^1.2.8",
    "@externs/goa": "^0.1.0",
    "@goa/cookies": "^1.0.0",
    "@goa/koa": "^1.1.2",
    "@goa/uuid": "^1.0.1",
    "@idio/debug": "^1.1.1",
    "@typedefs/goa": "^0.0.2",
    "alamode": "^2.3.4",
    "documentary": "^1.27.3",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  }
}
