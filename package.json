{
  "name": "@goa/session",
  "version": "1.0.3",
  "description": "Session Middleware For Goa.",
  "main": "depack/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "lib": "depack src/depack -o compile/session.js -a -c -p --externs http",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js -u",
    "template": "typal compile/template.js -T compile/index.js -t types",
    "externs": "typal types/externs.js -e",
    "e": "alanode"
  },
  "files": [
    "depack",
    "src",
    "types"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/session.git"
  },
  "keywords": [
    "session",
    "goa"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/idiocc/session/issues"
  },
  "homepage": "https://www.idio.cc",
  "devDependencies": {
    "@contexts/http": "^1.6.1",
    "@externs/goa": "^0.1.3",
    "@goa/cookies": "^2.0.1",
    "@goa/koa": "^2.0.2",
    "@goa/uuid": "^1.0.2",
    "@idio/debug": "^1.2.3",
    "@typedefs/goa": "^0.1.3",
    "alamode": "^3.3.0",
    "depack": "^1.0.1",
    "documentary": "^1.35.3",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  }
}
